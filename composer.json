{
  "name": "xpbl4/yii2-grid",
  "description": "The GridView override for Yii2 framework.",
  "type": "yii2-extension",
  "keywords": ["yii2", "extension", "widget", "grid"],
  "homepage": "https://github.com/Xpbl4/yii2-grid",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Serge Mashkov",
      "email": "xpbl4@motorwars.ru",
      "homepage": "https://github.com/Xpbl4",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/Xpbl4/yii2-grid/issues?state=open",
    "source": "https://github.com/Xpbl4/yii2-grid"
  },
  "repositories":[
    {
      "type": "git",
      "url": "https://github.com/Xpbl4/yii2-grid.git"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.0"
  },
  "autoload": {
    "psr-4": {
      "xpbl4\\grid\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
